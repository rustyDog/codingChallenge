<div class="row">
  <div class="col-md-10 col-md-offset-1 jumbotron">
    <form class="form form-inline">
      <div class="row">
        <div class="col-md-8 col-md-offset-2">
          <div class="form-group">
            {{input type="text" placeholder="Member Number" id="memberNumber" class="form-control" value=memberNumber}}
          </div>
          <div class="form-group col-sm-offset-1">
            {{input type="text" placeholder="First Name" id="firstName" class="form-control" value=firstName}}
          </div>
          <div class="form-group col-sm-offset-1">
            {{input type="text" placeholder="Last Name" id="lastName" class="form-control" value=lastName}}
          </div>
        </div>
      </div>

      <div class="row"><br/></div>

      <div class="row">

        <div class="col-md-8 col-md-offset-2">
          <div class="form-group">
            {{input type="text" placeholder="Date of Birth" id="dob" class="form-control" value=dob}}
          </div>
          <div class="form-group col-sm-offset-1">
            {{input type="text" placeholder="email" id="email" class="form-control" value=email}}
          </div>
          <div class="form-group col-sm-offset-1">
            {{input type="text" placeholder="Phone #" id="homePhone" class="form-control" value=homePhone}}
          </div>
        </div>
      </div>

      <div class="row">
        <div class="col-md-2 col-md-offset-10">
          <div class="form-group">
            <button class="btn btn-primary" value="Submit" type="submit" {{action 'performSearch'}}>Submit</button>
          </div>
        </div>
      </div>
    </form>
  </div>
</div>
